	cd ./iomod && $(MAKE) clean
	cd ./timemod && $(MAKE) clean
	cd ./filemod && $(MAKE) clean
	cd ./sqldrivers/mysqlmod && $(MAKE) clean
	cd ./mathmod && $(MAKE) clean
	cd ./sessionmod && $(MAKE) clean
	cd ./networkmod && $(MAKE) clean
	cd ./processmod && $(MAKE) clean


