all:
	cd ./iomod && $(MAKE) standalone
	cd ./timemod && $(MAKE) standalone
	cd ./filemod && $(MAKE) standalone
	cd ./sqldrivers/mysqlmod && $(MAKE) standalone
clean:
	cd ./iomod && $(MAKE) clean
	cd ./timemod && $(MAKE) clean
	cd ./filemod && $(MAKE) clean
	cd ./sqldrivers/mysqlmod && $(MAKE) clean
